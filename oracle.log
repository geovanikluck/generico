1083        i integer;
1084        j integer;
1085        s substitution;
1086     begin
1087        j := 1;
1088        i := 1;
1089        while (i <= length(arg))
1090        loop
1091           if ind(arg,i) = AMP
1092           then
1093              addpat(DITTO, s, j);
1094           else
1095              escsub(arg, i, s, j);
1096           end if;
1097  
1098           i := i + 1;
1099        end loop;
1100  
1101        addpat(EOP, s, j);
