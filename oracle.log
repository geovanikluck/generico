830     procedure br_backoff(pat    in out pattern,
831                          j      in     integer,
832                          lastbr in out integer) is
833     begin
834        while (lastbr > j)
835        loop
836           pat(lastbr + LOC_IND) := 0;
837           lastbr := pat(lastbr + PREVBR_IND);
838        end loop;
839     end;
