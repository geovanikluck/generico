 17    IF pf_id = PLATFORM_WINDOWS32 THEN
 18      not_win := FALSE;
 19    ELSIF pf_id = PLATFORM_WINDOWSIA64 THEN
 20      not_win := FALSE;
 21    ELSIF pf_id = PLATFORM_WINDOWS64 THEN
 22      not_win := FALSE;
 23    END IF;
 24  
 25    :opatch_inst_dir := :opatch_script_dir;
 26    IF not_win THEN
 27      :opatch_script_dir := :opatch_script_dir || '/QOpatch';
