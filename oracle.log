185     function esc(arg in varchar2, i in out integer) return varchar2
186        is
187        NL_ESCAPE  constant varchar2(1) := 'n';
188        TAB_ESCAPE constant varchar2(1) := 't';
189  
190        arg_i varchar2(5); -- MAX NO OF BYTES FOR NLS CHARSET
191     begin
192        arg_i := ind(arg,i);
193        if (arg_i != ESCAPE)
194        then
195           return arg_i;
196        end if;
197  
198        if (i = length(arg))
199        then
200           return arg_i;
201        end if;
202  
203        i := i+1;
204        arg_i := ind(arg,i);
205        if (arg_i = NL_ESCAPE)
206        then
207           return NEWLINE;
208        end if;
209  
210        if (arg_i = TAB_ESCAPE)
211        then
212           return TAB;
213        end if;
214  
215        return arg_i;
216     end;
