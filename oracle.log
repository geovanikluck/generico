115     function base_convert(str in varchar2, base in integer) return integer
116       is
117        acc integer;
118        len integer;
119     begin
120        len := length(str);
121        if (len is NULL)
122        then
123           return NULL;
124        end if;
125  
126        acc := 0;
127        for i in 1..len
128        loop
129           if (instr('abcdefABCDEF', ind(str,i)) != 0)
130           then
131              acc := acc*base + ascii(convert(lower(ind(str,i)), 'US7ASCII')) - 87;
132           else
133              acc := acc*base + ind(str,i);
134           end if;
135        end loop;
136  
137        return acc;
138     end;
