 53        END IF;
 54  
 55        select count(*) into l_priv_cnt from dba_tab_privs where
 56          GRANTEE ='ORACLE_OCM' and TABLE_NAME='UTL_INADDR' and
 57          upper(PRIVILEGE) = 'EXECUTE';
