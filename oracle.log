136  
137        if (OWA_CUSTOM.DBMS_SERVER_GMTDIFF is not NULL)
138        then
139           expires_gmt := expires-(OWA_CUSTOM.DBMS_SERVER_GMTDIFF/24);
140        else
141           expires_gmt := new_time(expires,OWA_CUSTOM.DBMS_SERVER_TIMEZONE,'GMT');
142        end if;
