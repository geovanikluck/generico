172     procedure copypat(pat in out pattern,
173                       j   in out integer,
174                       loc in     integer) is
175        pat_size integer;
176        k    integer;
177     begin
178        pat_size := patsize(pat,loc);
179        for k in loc..loc+pat_size-1
180        loop
181           addpat(pat(k), pat, j);
182        end loop;
183     end;
